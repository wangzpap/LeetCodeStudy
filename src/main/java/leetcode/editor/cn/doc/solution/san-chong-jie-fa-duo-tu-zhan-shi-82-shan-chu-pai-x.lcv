class Solution(object):
    def deleteDuplicates(self, head):
        if not (head and head.next):
            return head
        dummy = ListNode(-1)
        dummy.next = head
        a = dummy
        b = head.next
        while b:
            if a.next.val!=b.val:
                a = a.next
                b = b.next
            else:
                while b and a.next.val==b.val:
                    b = b.next
                # è¿™é‡Œçš„å»é‡è·Ÿè§£æ³•äºŒæœ‰ç‚¹å·®åˆ«ï¼Œè§£æ³•äºŒçš„æ˜¯

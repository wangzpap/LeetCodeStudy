class Solution {
    public ListNode deleteDuplicates(ListNode head) {
        if(head==null || head.next==null) {
            return head;
        }
        ListNode dummy = new ListNode(-1);
        dummy.next = head;
        ListNode a = dummy;
        ListNode b = head.next;
        while(b!=null) {
            if(a.next.val!=b.val) {
                a = a.next;
                b = b.next;
            }
            else {
                while(b!=null && a.next.val==b.val) {
                    b = b.next;
                }
                //è¿™é‡Œçš„å»é‡è·Ÿè§£æ³•äºŒæœ‰ç‚¹å·®åˆ«ï¼Œè§£æ³•äºŒçš„æ˜¯
                //a.next = b.next
                a.next = b;

    def deleteDuplicates(self, head):
        if not (head and head.next):
            return head
        dummy = ListNode(-1)
        dummy.next = head
        a = dummy
        b = head
        while b and b.next:
            # åˆå§‹åŒ–çš„æ—¶aæŒ‡å‘çš„æ˜¯å“‘ç»“ç‚¹ï¼Œæ‰€ä»¥æ¯”è¾ƒé€»è¾‘åº”è¯¥æ˜¯açš„ä¸‹ä¸€ä¸ªèŠ‚ç‚¹å’Œbçš„ä¸‹ä¸€ä¸ªèŠ‚ç‚¹
            if a.next.val!=b.next.val:
